{
  "name": "klogs",
  "version": "0.0.1",
  "main": "app.ts",
  "description": "App Para Visualizar Logs de Aplicações Num Cluster Kubernetes",
  "author": "Jose Ricardo Dainese <josericardodainese@gmail.com>",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "release": "electron-forge publish"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~12.2.0",
    "@angular/cdk": "^12.2.7",
    "@angular/common": "~12.2.0",
    "@angular/compiler": "~12.2.0",
    "@angular/core": "~12.2.0",
    "@angular/forms": "~12.2.0",
    "@angular/material": "^12.2.7",
    "@angular/platform-browser": "~12.2.0",
    "@angular/platform-browser-dynamic": "~12.2.0",
    "@angular/router": "~12.2.0",
    "@types/prismjs": "^1.9.0",
    "electron-squirrel-startup": "^1.0.0",
    "ngx-highlight-js": "^12.0.0",
    "normalize.css": "^8.0.1",
    "prismjs": "^1.25.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.2.7",
    "@angular/cli": "~12.2.7",
    "@angular/compiler-cli": "~12.2.0",
    "@electron-forge/cli": "^6.0.0-beta.61",
    "@electron-forge/maker-deb": "^6.0.0-beta.61",
    "@electron-forge/maker-rpm": "^6.0.0-beta.61",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.61",
    "@electron-forge/maker-zip": "^6.0.0-beta.61",
    "@types/jasmine": "~3.8.0",
    "@types/node": "^12.11.1",
    "browser-sync": "^2.27.5",
    "electron": "^15.0.0",
    "electron-installer-windows": "^3.0.0",
    "jasmine-core": "~3.8.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "typescript": "~4.3.5"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "KLogs",
        "executableName": "klogs",
        "icon": "buils/icon",
        "extraResource": [
          "build"
        ]
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "klogs"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "publishers": [
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "josericardodainese",
              "name": "klogs"
            },
            "draft": true
          }
        }
      ]
    }
  }
}
